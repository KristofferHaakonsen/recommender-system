im."]}, {"field": "category0", "value": "kultur"}, {"field": "contenthash", "value": "f8be476c45fde7b01b8a463d164fd9a769dbb6512323da52737c4f5c0b2bce75"}, {"field": "createtime", "value": "2015-12-19T20:58:06.000Z"}, {"field": "description", "value": "I dag fikk 60-tallets kollektive forelskelse permanent utstilling p\u00e5 Rockheim."}, {"field": "dominantimage", "value": "http://www.adressa.no/incoming/article6967803.ece/ALTERNATES/w980-default/Grynet%20Molvig%202.jpg"}, {"field": "dominantimagedimensions", "value": "980x551"}, {"field": "dominantthumbnail", "value": "http://content-thumbnail.cxpublic.com/content/dominantthumbnail/ccf72096d6e0980c29e6942d3351c250ea1ef8b8.jpg?5992d1ee"}, {"field": "dominantthumbnaildimensions", "value": "300x169"}, {"field": "heading", "value": ["Husker du denne dama? -adressa.no", "Oppkalt etter et lam", "Glemt i hjemlandet"]}, {"field": "id", "value": "ccf72096d6e0980c29e6942d3351c250ea1ef8b8"}, {"field": "keyword", "value": ["anmeldelser", "det skjer", "film", "musikk", "tv", "museum", "scene", "spill"]}, {"field": "keywords", "value": "anmeldelser,det skjer,film,musikk,tv,museum,scene,spill"}, {"field": "kw-category", "value": "kultur"}, {"field": "kw-classification", "value": "arts-and-entertainment"}, {"field": "kw-concept", "value": ["skuespiller", "popul\u00e6r", "utstilling", "forelsket", "kjent", "film", "dama"]}, {"field": "kw-entity", "value": ["grynet molvig", "stompa", "s\u00f8nner", "rockheim", "ser du jan", "stockholms", "anna-kristin"]}, {"field": "kw-location", "value": ["norge", "sverige"]}, {"field": "kw-pageclass", "value": "article"}, {"field": "kw-person", "value": "terje nilsen"}, {"field": "kw-taxonomy", "value": "kultur"}, {"field": "og-image", "value": "http://www.adressa.no/incoming/article6967803.ece/ALTERNATES/w980-default/Grynet%20Molvig%202.jpg"}, {"field": "og-locale", "value": "nb_NO"}, {"field": "og-site-name", "value": "adressa.no"}, {"field": "og-title", "value": "Husker du denne dama?"}, {"field"