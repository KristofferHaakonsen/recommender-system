ngssaken."]}, {"field": "category0", "value": "nyheter"}, {"field": "contenthash", "value": "6aef74dc78ad48abd936422e5b1c3d1243dfd3b546b7ff4e1df3ca032ba4c53f"}, {"field": "createtime", "value": "2017-07-29T09:57:43.000Z"}, {"field": "description", "value": "En to meter h\u00f8y giraff ble stj\u00e5let fra Solsiden kj\u00f8pesenter f\u00f8r helga. Uten at noen tok notis av det, ble det to meter h\u00f8ye dyret bortf\u00f8rt uten selv \u00e5 gj\u00f8re motstand."}, {"field": "dominantimage", "value": "http://www.adressa.no/incoming/article1035895.ece5/ALTERNATES/w980-default/ver2%20QD0271003_250"}, {"field": "dominantimagedimensions", "value": "980x551"}, {"field": "dominantthumbnail", "value": "http://content-thumbnail.cxpublic.com/content/dominantthumbnail/32e411b018c0013b307b1ca7abdd1fdc64fe8ba0.jpg?597c5c17"}, {"field": "dominantthumbnaildimensions", "value": "300x169"}, {"field": "heading", "value": "Har noen sett en giraff? -adressa.no"}, {"field": "id", "value": "32e411b018c0013b307b1ca7abdd1fdc64fe8ba0"}, {"field": "keyword", "value": ["utenriks", "innenriks", "trondheim", "E6", "midtbyen", "bybrann", "bilulykker"]}, {"field": "keywords", "value": "utenriks,innenriks,trondheim,E6,midtbyen,bybrann,bilulykker"}, {"field": "kw-category", "value": "nyheter"}, {"field": "kw-concept", "value": "giraff"}, {"field": "kw-entity", "value": ["edwis", "gira", "solsiden"]}, {"field": "kw-pageclass", "value": "article"}, {"field": "kw-person", "value": ["bente g\u00e4tzschmann", "heidi storbr\u00e5ten"]}, {"field": "kw-taxonomy", "value": "nyheter"}, {"field": "og-image", "value": "http://www.adressa.no/incoming/article1035895.ece5/ALTERNATES/w980-default/ver2%20QD0271003_250"}, {"field": "og-locale", "value": "nb_NO"}, {"field": "og-site-name", "value": "adressa.no"}, {"field": "og-title", "value": "Har noen sett en giraff?"}, {"field": "og-type", "value": "article"}, {"field": "og-url", "value": "http://www.adressa.no/nyheter/article174990.ece"}, {"field": "pageclass", "v