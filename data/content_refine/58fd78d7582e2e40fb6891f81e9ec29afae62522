 minutt."]}, {"field": "category0", "value": "nyheter"}, {"field": "category1", "value": "nyheter|kuriosa"}, {"field": "contenthash", "value": "196af32cb96cb95120836fddfd19f1bfd2ba8be7b73adac2a04778faec91cdfe"}, {"field": "createtime", "value": "2014-06-10T13:52:52.772Z"}, {"field": "description", "value": "Amerikaneren Ashrita Furman satte ny verdensrekord ved \u00e5 ha 100 oppf\u00f8ringer i Guinness rekordbok samtidig."}, {"field": "dominantimage", "value": "http://www.adressa.no/incoming/article1321341.ece5/ALTERNATES/w980-default/furman1_16-9"}, {"field": "dominantimagedimensions", "value": "980x551"}, {"field": "dominantthumbnail", "value": "http://content-thumbnail.cxpublic.com/content/dominantthumbnail/58fd78d7582e2e40fb6891f81e9ec29afae62522.jpg?59874710"}, {"field": "dominantthumbnaildimensions", "value": "300x169"}, {"field": "heading", "value": ["Ny verdensrekord: har flest rekorder -adressa.no", "Ikke interessert i fysisk aktivitet", "Flest, lengst, raskest"]}, {"field": "id", "value": "58fd78d7582e2e40fb6891f81e9ec29afae62522"}, {"field": "keyword", "value": ["utenriks", "innenriks", "trondheim", "E6", "midtbyen", "bybrann", "bilulykker"]}, {"field": "keywords", "value": "utenriks,innenriks,trondheim,E6,midtbyen,bybrann,bilulykker"}, {"field": "kw-category", "value": ["nyheter", "kuriosa"]}, {"field": "kw-classification", "value": "sports"}, {"field": "kw-concept", "value": ["flest rekorder", "rekord", "flest"]}, {"field": "kw-entity", "value": ["guinness", "ashrita furman", "sri chinmoy", "the daily telegraph"]}, {"field": "kw-location", "value": "new york"}, {"field": "kw-pageclass", "value": "article"}, {"field": "kw-person", "value": "mr. versatility"}, {"field": "kw-taxonomy", "value": ["nyheter", "nyheter/kuriosa"]}, {"field": "og-image", "value": "http://www.adressa.no/incoming/article1321341.ece5/ALTERNATES/w980-default/furman1_16-9"}, {"field": "og-locale", "value": "nb_NO"}, {"field": "og-site-name", "value": "adressa.no"}, {"field": "og-titl