ig."]}, {"field": "category0", "value": "nyheter"}, {"field": "category1", "value": "nyheter|innenriks"}, {"field": "contenthash", "value": "f9fb00a8f7da92f894bb87a23b59f0b1e07a084cee5affcd69bd7beb4a39c2cb"}, {"field": "createtime", "value": "2017-05-01T01:02:41.000Z"}, {"field": "description", "value": "Svindlerne lokker med Rema-gavekort av h\u00f8y verdi i e-post."}, {"field": "dominantimage", "value": "http://www.adressa.no/nyheter/innenriks/article11940713.ece/f8ognh/ALTERNATES/w980-default/remabilde.jpg"}, {"field": "dominantimagedimensions", "value": "980x551"}, {"field": "dominantthumbnail", "value": "http://content-thumbnail.cxpublic.com/content/dominantthumbnail/021e41678c55c81c06aedf282e71a7c67f33d405.jpg?5984713b"}, {"field": "dominantthumbnaildimensions", "value": "300x169"}, {"field": "heading", "value": ["Rema 1000 misbrukes i gavekortsvindel -adressa.no", "Sender tilbud om \u00e5 delta i konkurranse", "- Ikke besvar henvendelsene"]}, {"field": "id", "value": "021e41678c55c81c06aedf282e71a7c67f33d405"}, {"field": "keyword", "value": ["utenriks", "innenriks", "trondheim", "E6", "midtbyen", "bybrann", "bilulykker"]}, {"field": "keywords", "value": "utenriks,innenriks,trondheim,E6,midtbyen,bybrann,bilulykker"}, {"field": "kw-category", "value": ["nyheter", "innenriks"]}, {"field": "kw-company", "value": "facebook"}, {"field": "kw-concept", "value": ["svindlere lokker", "sosiale medier", "falsk", "rema", "svindlere"]}, {"field": "kw-entity", "value": "rema 1000"}, {"field": "kw-pageclass", "value": "article"}, {"field": "kw-person", "value": "amalie knudsen"}, {"field": "kw-sentiment", "value": "negative"}, {"field": "kw-taxonomy", "value": ["nyheter", "nyheter/innenriks"]}, {"field": "og-image", "value": "http://www.adressa.no/nyheter/innenriks/article11940713.ece/f8ognh/ALTERNATES/w980-default/remabilde.jpg"}, {"field": "og-locale", "value": "nb_NO"}, {"field": "og-site-name", "value": "adressa.no"}, {"field": "og-title", "value": "Rema 1000 misbrukes i