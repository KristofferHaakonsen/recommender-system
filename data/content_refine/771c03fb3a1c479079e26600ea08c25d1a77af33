n."]}, {"field": "category0", "value": "100sport"}, {"field": "category1", "value": "100sport|fotball"}, {"field": "contenthash", "value": "b5ae0036df3f6a15b1c1775ff6c98a326ec4a936a3533a66b498c34689a80585"}, {"field": "createtime", "value": "2017-02-08T11:30:01.000Z"}, {"field": "description", "value": "\u2014 Tror vi klarer \u00e5 passere grensen, sier RBKs daglige leder."}, {"field": "heading", "value": ["N\u00e6rmer seg \u00abmagisk grense\u00bb for salg av sesongkort -adressa.no", "8300 i fjor", "Jager 20 000-m\u00e5let"]}, {"field": "id", "value": "771c03fb3a1c479079e26600ea08c25d1a77af33"}, {"field": "keyword", "value": ["Eliteserien", "Rosenborg BK"]}, {"field": "keywords", "value": "Eliteserien,Rosenborg BK"}, {"field": "kw-category", "value": ["fotball", "100sport"]}, {"field": "kw-classification", "value": "sports"}, {"field": "kw-concept", "value": ["magisk grense", "dager", "daglig leder", "salg", "sesongkort", "grense"]}, {"field": "kw-entity", "value": ["dyrhaug", "godset", "rosenborg bk"]}, {"field": "kw-pageclass", "value": "article"}, {"field": "kw-person", "value": ["moe dyrhaug", "tove moe dyrhaug"]}, {"field": "kw-taxonomy", "value": ["100sport", "100sport/fotball"]}, {"field": "og-image", "value": "http://vertical.snd.no/images/ddbd25ec-acf1-4d0d-92be-c758511a939d?fit=crop&h=551&q=80&w=980"}, {"field": "og-locale", "value": "nb_NO"}, {"field": "og-site-name", "value": "adressa.no"}, {"field": "og-title", "value": "N\u00e6rmer seg \u00abmagisk grense\u00bb for salg av sesongkort"}, {"field": "og-type", "value": "article"}, {"field": "og-url", "value": "http://www.adressa.no/100Sport/fotball/Narmer-seg-magisk-grense-for-salg-av-sesongkort-214099b.html"}, {"field": "pageclass", "value": ["article"]}, {"field": "publishtime", "value": "2016-03-17T17:48:00.000Z"}, {"field": "recs-articleid", "value": "214099"}, {"field": "recs-category-type", "value": "taxonomy"}, {"field": "recs-category0", "value": "taxonomy|100sport"}, {"field": "recs-category1", 