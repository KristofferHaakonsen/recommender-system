ag."}, {"field": "dominantimage", "value": "http://static.polarismedia.no/skins/prod/publication/adressa/gfx/applefavicon.png"}, {"field": "dominantimagedimensions", "value": "256x256"}, {"field": "dominantthumbnail", "value": "http://content-thumbnail.cxpublic.com/content/dominantthumbnail/0e3f3b42b781f66ae8da20258d7ca2ec263f4e95.jpg?59516c84"}, {"field": "dominantthumbnaildimensions", "value": "256x256"}, {"field": "heading", "value": "D\u00f8de i familieselskap -adressa.no"}, {"field": "id", "value": "0e3f3b42b781f66ae8da20258d7ca2ec263f4e95"}, {"field": "keyword", "value": ["utenriks", "innenriks", "trondheim", "E6", "midtbyen", "bybrann", "bilulykker"]}, {"field": "keywords", "value": "utenriks,innenriks,trondheim,E6,midtbyen,bybrann,bilulykker"}, {"field": "kw-category", "value": "nyheter"}, {"field": "kw-classification", "value": "law-government-and-politics"}, {"field": "kw-concept", "value": ["politiet", "d\u00f8d", "badestamp", "familieselskap", "kvinnen"]}, {"field": "kw-entity", "value": ["ytre namdal", "elvalandet"]}, {"field": "kw-location", "value": "namsos"}, {"field": "kw-pageclass", "value": "article"}, {"field": "kw-person", "value": ["asbj\u00f8rn t\u00f8nne", "trine myrvang"]}, {"field": "kw-sentiment", "value": "negative"}, {"field": "kw-taxonomy", "value": "nyheter"}, {"field": "og-image", "value": "http://static.polarismedia.no/skins/prod/publication/adressa/gfx/applefavicon.png"}, {"field": "og-locale", "value": "nb_NO"}, {"field": "og-site-name", "value": "adressa.no"}, {"field": "og-title", "value": "D\u00f8de i familieselskap"}, {"field": "og-type", "value": "article"}, {"field": "og-url", "value": "http://www.adressa.no/nyheter/article50089.ece"}, {"field": "pageclass", "value": ["article"]}, {"field": "publishtime", "value": "2003-09-01T13:40:47.000Z"}, {"field": "recs-articleid", "value": "901911"}, {"field": "recs-category-type", "value": "taxonomy"}, {"field": "recs-category0", "value": "taxonomy|nyheter"}, {"field": "recs-publishtim