 seg bak."]}, {"field": "category0", "value": "nyheter"}, {"field": "category1", "value": "nyheter|sortrondelag"}, {"field": "contenthash", "value": "d8caebe13895e5c2c35b30d0d6b1fb91226959168bc2e5b5d9db76802faaff3c"}, {"field": "createtime", "value": "2017-04-30T19:22:35.000Z"}, {"field": "description", "value": "Forollhogna Nasjonalpark blir v\u00e6rende yngleomr\u00e5de for jerv. Naturvernforbundet er sv\u00e6rt forn\u00f8yd med avgj\u00f8relsen."}, {"field": "dominantimage", "value": "http://www.adressa.no/nyheter/article10775364.ece/2h02lk/ALTERNATES/w980-default/IMG_Nasjonalparker003.jp_1_1_4L5I149.jpg"}, {"field": "dominantimagedimensions", "value": "980x551"}, {"field": "dominantthumbnail", "value": "http://content-thumbnail.cxpublic.com/content/dominantthumbnail/8d046a86c7ff36654cbc8301e5c7232e4b49c8cb.jpg?5973c116"}, {"field": "dominantthumbnaildimensions", "value": "300x169"}, {"field": "heading", "value": "Jerven f\u00e5r yngle i Forollhogna -adressa.no"}, {"field": "id", "value": "8d046a86c7ff36654cbc8301e5c7232e4b49c8cb"}, {"field": "keyword", "value": ["utenriks", "innenriks", "trondheim", "E6", "midtbyen", "bybrann", "bilulykker"]}, {"field": "keywords", "value": "utenriks,innenriks,trondheim,E6,midtbyen,bybrann,bilulykker"}, {"field": "kw-category", "value": ["nyheter", "sortrondelag"]}, {"field": "kw-classification", "value": "law-government-and-politics"}, {"field": "kw-concept", "value": ["yngleomr\u00e5de", "jerv"]}, {"field": "kw-entity", "value": ["forollhogna", "per flatberg", "scandic hell hotell", "nasjonalpark", "naturvernforbundet", "norsk", "s\u00f8r-norge", "per flatberg og \u00f8ystein folden", "bioforsk og norges", "flatberg"]}, {"field": "kw-location", "value": ["s\u00f8r-tr\u00f8ndelag", "stj\u00f8rdal"]}, {"field": "kw-pageclass", "value": "article"}, {"field": "kw-person", "value": ["john linnell", "lars haltbrekken", "\u00f8ystein folden"]}, {"field": "kw-taxonomy", "value": ["nyheter", "nyheter/sortrondelag"]}, {"field": "og-imag