."]}, {"field": "category0", "value": "nyheter"}, {"field": "category1", "value": "nyheter|nordtrondelag"}, {"field": "contenthash", "value": "0a7e960fb97c024be6b3d72fb20f3866caf08aa5b288935746ef1b8f0baace5e"}, {"field": "createtime", "value": "2017-04-30T09:07:18.000Z"}, {"field": "description", "value": "Direktoratet for naturforvaltning (DN) ber publikum melde fra om de ser en m\u00e5rhund."}, {"field": "dominantimage", "value": "http://www.adressa.no/incoming/article1341669.ece5/ALTERNATES/w980-default/100124_maarhund"}, {"field": "dominantimagedimensions", "value": "980x551"}, {"field": "dominantthumbnail", "value": "http://content-thumbnail.cxpublic.com/content/dominantthumbnail/3eda2539c086ef8eb085342e568cb1a537b49087.jpg?599966a1"}, {"field": "dominantthumbnaildimensions", "value": "300x169"}, {"field": "heading", "value": ["Ber folk varsle om de ser m\u00e5rhund -adressa.no", "Analysegrunnlag", "Stor overlevelsesevne"]}, {"field": "id", "value": "3eda2539c086ef8eb085342e568cb1a537b49087"}, {"field": "keyword", "value": ["utenriks", "innenriks", "trondheim", "E6", "midtbyen", "bybrann", "bilulykker"]}, {"field": "keywords", "value": "utenriks,innenriks,trondheim,E6,midtbyen,bybrann,bilulykker"}, {"field": "kw-category", "value": ["nyheter", "nordtrondelag"]}, {"field": "kw-classification", "value": "pets"}, {"field": "kw-concept", "value": "m\u00e5rhund"}, {"field": "kw-entity", "value": ["finnmark og troms", "oppland og hedmark", "bioforsk", "nordland og nord-tr\u00f8ndelag", "h\u00f8ylandet film", "fylkesmannen", "nationen", "rovdata", "statens"]}, {"field": "kw-location", "value": ["s\u00f8r-tr\u00f8ndelag", "norden", "sverige", "finnmark", "nord-tr\u00f8ndelag", "\u00e5land", "norge", "leka", "finland"]}, {"field": "kw-pageclass", "value": "article"}, {"field": "kw-person", "value": ["janne solli", "erik lund", "lornts m\u00f8rkved"]}, {"field": "kw-taxonomy", "value": ["nyheter", "nyheter/nordtrondelag"]}, {"field": "og-image", "value": "http://www.adre