visen."]}, {"field": "category0", "value": "nyheter"}, {"field": "contenthash", "value": "64a1a3ef9cf9dcb789544eea95dcfa0de9b7d99d8b42b7a1783fdb69ff9661d7"}, {"field": "createtime", "value": "2016-01-27T17:09:58.000Z"}, {"field": "description", "value": "Frode Stormyr forteller i denne videoen sin versjon av hva som skjedde p\u00e5 Br\u00f8set en dag i 2010. Og hva som skjedde da han varslet."}, {"field": "dominantimage", "value": "http://www.adressa.no/incoming/article9094619.ece/ALTERNATES/w980-default/Br%C3%B8set-16-9-210114%20copy.jpg"}, {"field": "dominantimagedimensions", "value": "980x551"}, {"field": "dominantthumbnail", "value": "http://content-thumbnail.cxpublic.com/content/dominantthumbnail/b3c89e0849d7ce115231bbbfe5aa22a8f30ee185.jpg?596a5ce7"}, {"field": "dominantthumbnaildimensions", "value": "300x169"}, {"field": "heading", "value": "H\u00f8r varsleren fortelle -adressa.no"}, {"field": "id", "value": "b3c89e0849d7ce115231bbbfe5aa22a8f30ee185"}, {"field": "keyword", "value": ["utenriks", "innenriks", "trondheim", "E6", "midtbyen", "bybrann", "bilulykker"]}, {"field": "keywords", "value": "utenriks,innenriks,trondheim,E6,midtbyen,bybrann,bilulykker"}, {"field": "kw-category", "value": "nyheter"}, {"field": "kw-classification", "value": "education"}, {"field": "kw-concept", "value": ["pasient", "varsleren fortelle", "skjedde", "sak", "hendelsen", "varslet"]}, {"field": "kw-entity", "value": ["br\u00f8set", "fylkesmannen", "kontrollkommisjon", "adresseavisen", "olavs hospital", "st. olavs hospital"]}, {"field": "kw-pageclass", "value": "article"}, {"field": "kw-person", "value": "frode stormyr"}, {"field": "kw-taxonomy", "value": "nyheter"}, {"field": "og-image", "value": "http://www.adressa.no/incoming/article9094619.ece/ALTERNATES/w980-default/Br%C3%B8set-16-9-210114%20copy.jpg"}, {"field": "og-locale", "value": "nb_NO"}, {"field": "og-site-name", "value": "adressa.no"}, {"field": "og-title", "value": "H\u00f8r varsleren fortelle"}, {"field": "og-type",