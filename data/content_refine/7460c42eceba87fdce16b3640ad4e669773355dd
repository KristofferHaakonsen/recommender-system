ars Vaular"]}, {"field": "category0", "value": "kultur"}, {"field": "contenthash", "value": "ebb7269b57d4d64aa9b08ef846504a6fd230eb21db72ab86e4fbf920fe8bfd63"}, {"field": "createtime", "value": "2017-07-08T15:40:14.000Z"}, {"field": "description", "value": "- Det er ikke Manu Chao som er headliner, det er Tr\u00e6na selv som er det."}, {"field": "dominantimage", "value": "http://www.adressa.no/incoming/article1398049.ece5/ALTERNATES/w980-default/tr%C3%A6na092"}, {"field": "dominantimagedimensions", "value": "980x551"}, {"field": "dominantthumbnail", "value": "http://content-thumbnail.cxpublic.com/content/dominantthumbnail/7460c42eceba87fdce16b3640ad4e669773355dd.jpg?5960fcde"}, {"field": "dominantthumbnaildimensions", "value": "300x169"}, {"field": "heading", "value": ["Tr\u00f8ndere p\u00e5 Tr\u00e6na -adressa.no", "- Stille f\u00f8r stormen", "Utsolgt"]}, {"field": "id", "value": "7460c42eceba87fdce16b3640ad4e669773355dd"}, {"field": "keyword", "value": ["anmeldelser", "det skjer", "film", "musikk", "tv", "museum", "scene", "spill"]}, {"field": "keywords", "value": "anmeldelser,det skjer,film,musikk,tv,museum,scene,spill"}, {"field": "kw-category", "value": "kultur"}, {"field": "kw-classification", "value": "arts-and-entertainment"}, {"field": "kw-concept", "value": ["tr\u00f8ndere", "festivalen"]}, {"field": "kw-entity", "value": ["tr\u00e6naminne", "tr\u00e6nafestivalen", "nordlendingen"]}, {"field": "kw-location", "value": ["sandnessj\u00f8en", "tr\u00e6na", "nordland"]}, {"field": "kw-pageclass", "value": "article"}, {"field": "kw-person", "value": ["h\u00e5vard heringstad", "manu chao"]}, {"field": "kw-taxonomy", "value": "kultur"}, {"field": "og-image", "value": "http://www.adressa.no/incoming/article1398049.ece5/ALTERNATES/w980-default/tr%C3%A6na092"}, {"field": "og-locale", "value": "nb_NO"}, {"field": "og-site-name", "value": "adressa.no"}, {"field": "og-title", "value": "Tr\u00f8ndere p\u00e5 Tr\u00e6na"}, {"field": "og-type", "value": "article"}, {"fiel