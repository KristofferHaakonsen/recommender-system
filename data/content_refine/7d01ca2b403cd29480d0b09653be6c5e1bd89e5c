n."]}, {"field": "category0", "value": "nyheter"}, {"field": "category1", "value": "nyheter|trondheim"}, {"field": "contenthash", "value": "1c707ac26b02e4850e15c587f86fafb09869d05dfda195be330de7d731794b18"}, {"field": "createtime", "value": "2014-10-08T08:03:02.440Z"}, {"field": "description", "value": "Ocean Space Centre i Trondheim er et av to verdensledende milj\u00f8er som pekes ut som store byggeprosjekter i regjeringens langtidsplan for forskning."}, {"field": "dominantimage", "value": "http://www.adressa.no/incoming/article10209847.ece/8k4kv1/ALTERNATES/w980-default/Space%20Center"}, {"field": "dominantimagedimensions", "value": "980x551"}, {"field": "dominantthumbnail", "value": "http://content-thumbnail.cxpublic.com/content/dominantthumbnail/7d01ca2b403cd29480d0b09653be6c5e1bd89e5c.jpg?59706282"}, {"field": "dominantthumbnaildimensions", "value": "300x169"}, {"field": "heading", "value": "Regjeringen satser p\u00e5 Ocean Space Centre -adressa.no"}, {"field": "id", "value": "7d01ca2b403cd29480d0b09653be6c5e1bd89e5c"}, {"field": "keyword", "value": ["utenriks", "innenriks", "trondheim", "E6", "midtbyen", "bybrann", "bilulykker"]}, {"field": "keywords", "value": "utenriks,innenriks,trondheim,E6,midtbyen,bybrann,bilulykker"}, {"field": "kw-category", "value": ["nyheter", "trondheim"]}, {"field": "kw-classification", "value": "law-government-and-politics"}, {"field": "kw-concept", "value": ["regjeringen satser", "regjeringen", "trondheim"]}, {"field": "kw-entity", "value": ["h\u00f8yre", "ocean space centre", "life science centre", "frp", "ocean space center", "ocean space", "tyholt", "fremskrittspartiets", "solberg", "trondheim og life science centre"]}, {"field": "kw-location", "value": ["trondheim", "oslo"]}, {"field": "kw-pageclass", "value": "article"}, {"field": "kw-person", "value": ["torbj\u00f8rn r\u00f8e isaksen", "sivert bj\u00f8rnstad", "frank jenssen"]}, {"field": "kw-taxonomy", "value": ["nyheter", "nyheter/trondheim"]}, {"field": "og-image", "valu