 dag."}, {"field": "dominantimage", "value": "http://www.adressa.no/incoming/article11777659.ece/69yu8l/ALTERNATES/w980-default/jo%20lunder%201_2.jpg"}, {"field": "dominantimagedimensions", "value": "980x551"}, {"field": "dominantthumbnail", "value": "http://content-thumbnail.cxpublic.com/content/dominantthumbnail/09ca56826975422b7be55ea7d7dcc0f75abbb0bc.jpg?58e803a6"}, {"field": "dominantthumbnaildimensions", "value": "300x169"}, {"field": "heading", "value": ["Jo Lunder permitteres fra Fredriksen-gruppen -adressa.no", "Nekter", "Fengslingsm\u00f8te fredag", "Internasjonal etterforskning", "Styrelederen avsatt"]}, {"field": "id", "value": "09ca56826975422b7be55ea7d7dcc0f75abbb0bc"}, {"field": "keyword", "value": ["\u00f8konomi", "finans", "aksjer", "skatt", "b\u00f8rs", "\u00f8konominyheter"]}, {"field": "keywords", "value": "\u00f8konomi,finans,aksjer,skatt,b\u00f8rs,\u00f8konominyheter"}, {"field": "kw-category", "value": ["nyheter", "okonomi"]}, {"field": "kw-classification", "value": "business"}, {"field": "kw-company", "value": "telenor"}, {"field": "kw-concept", "value": ["inntil videre", "internasjonal etterforskning", "etterforsker"]}, {"field": "kw-entity", "value": ["stortingets", "gardermoen", "gulnara karimova", "jo lunder", "\u00f8kokrim", "vimpelcom", "n\u00e6ringsliv"]}, {"field": "kw-location", "value": ["gibraltar", "oslo", "sveits", "usbekistan"]}, {"field": "kw-pageclass", "value": "article"}, {"field": "kw-person", "value": ["monica m\u00e6land", "cato schi\u00f8tz", "harald thorstein", "marianne djupesland", "john fredriksen"]}, {"field": "kw-taxonomy", "value": ["nyheter", "nyheter/okonomi"]}, {"field": "og-image", "value": "http://www.adressa.no/incoming/article11777659.ece/69yu8l/ALTERNATES/w980-default/jo%20lunder%201_2.jpg"}, {"field": "og-locale", "value": "nb_NO"}, {"field": "og-site-name", "value": "adressa.no"}, {"field": "og-title", "value": "Jo Lunder permitteres fra Fredriksen-gruppen"}, {"field": "og-type", "value": "article"}, {"f