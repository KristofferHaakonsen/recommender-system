ere."}, {"field": "dominantimage", "value": "http://www.adressa.no/incoming/article1041379.ece5/ALTERNATES/w980-default/ver2%20oberst2"}, {"field": "dominantimagedimensions", "value": "980x551"}, {"field": "dominantthumbnail", "value": "http://content-thumbnail.cxpublic.com/content/dominantthumbnail/d74eb505524b5d758b69e9ec7247bb4ec1e94c9c.jpg?597b0e1d"}, {"field": "dominantthumbnaildimensions", "value": "300x169"}, {"field": "heading", "value": "Oppdaling sjef for kampskole -adressa.no"}, {"field": "id", "value": "d74eb505524b5d758b69e9ec7247bb4ec1e94c9c"}, {"field": "keyword", "value": ["utenriks", "innenriks", "trondheim", "E6", "midtbyen", "bybrann", "bilulykker"]}, {"field": "keywords", "value": "utenriks,innenriks,trondheim,E6,midtbyen,bybrann,bilulykker"}, {"field": "kw-category", "value": "nyheter"}, {"field": "kw-classification", "value": "education"}, {"field": "kw-concept", "value": ["h\u00f8yere niv\u00e5", "sjef", "kampskole"]}, {"field": "kw-entity", "value": ["h\u00e6rens", "kampv\u00e5penet", "brigaden", "united states army command", "united states army command and general staff college", "\u00f8sterdalen", "adresseavisen", "kosovo forces", "general staff college", "forsvarets"]}, {"field": "kw-location", "value": ["oppdal", "nord-norge", "rena", "telemark", "norge"]}, {"field": "kw-pageclass", "value": "article"}, {"field": "kw-person", "value": ["torstein johnson", "tor eystein s\u00e6ther"]}, {"field": "kw-taxonomy", "value": "nyheter"}, {"field": "og-image", "value": "http://www.adressa.no/incoming/article1041379.ece5/ALTERNATES/w980-default/ver2%20oberst2"}, {"field": "og-locale", "value": "nb_NO"}, {"field": "og-site-name", "value": "adressa.no"}, {"field": "og-title", "value": "Oppdaling sjef for kampskole"}, {"field": "og-type", "value": "article"}, {"field": "og-url", "value": "http://www.adressa.no/nyheter/article119902.ece"}, {"field": "pageclass", "value": ["article"]}, {"field": "publishtime", "value": "2002-06-21T08:03:01.000Z"}, {"fiel