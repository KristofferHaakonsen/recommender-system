."]}, {"field": "category0", "value": "sport"}, {"field": "category1", "value": "sport|langrenn"}, {"field": "contenthash", "value": "44b4f6635e793aa89e633e1bab45ad966eacb107b4a170521e1c097c469ca278"}, {"field": "createtime", "value": "2017-04-29T18:43:49.000Z"}, {"field": "description", "value": "Therese Johaug og co. har f\u00e5tt ny sponsoravtale med trondheimsbedrift."}, {"field": "dominantimage", "value": "http://www.adressa.no/incoming/article1394867.ece5/ALTERNATES/w980-default/johaugogco2"}, {"field": "dominantimagedimensions", "value": "980x551"}, {"field": "dominantthumbnail", "value": "http://content-thumbnail.cxpublic.com/content/dominantthumbnail/e6c2fa64217f75129ea729c0ecd385d531cf4475.jpg?5989da79"}, {"field": "dominantthumbnaildimensions", "value": "300x169"}, {"field": "heading", "value": "Skijenter ble jordb\u00e6rpiker -adressa.no"}, {"field": "id", "value": "e6c2fa64217f75129ea729c0ecd385d531cf4475"}, {"field": "keyword", "value": ["nyheter", "trondheim", "ukeadressa", "eadressa", "skattelister", "d\u00f8dsfall", "tr\u00f8nder", "trafikk"]}, {"field": "keywords", "value": "nyheter,trondheim,ukeadressa,eadressa,skattelister,d\u00f8dsfall,tr\u00f8nder,trafikk"}, {"field": "kw-category", "value": ["langrenn", "sport"]}, {"field": "kw-classification", "value": "sports"}, {"field": "kw-concept", "value": ["skijenter", "trondheim", "sponsoravtale"]}, {"field": "kw-entity", "value": ["s\u00f8rlandssenteret", "tveita senter", "marit kvaale og anita hegvik jakobsen", "kvaale og hegvik jakobsen", "marthe og kristin", "jordb\u00e6rpikene"]}, {"field": "kw-location", "value": ["melhus", "kristiansand", "oslo", "tr\u00f8ndelag", "trondheim", "drammen", "steinkjer", "norge"]}, {"field": "kw-pageclass", "value": "article"}, {"field": "kw-person", "value": ["marit kvaale", "anita hegvik jakobsen", "hegvik jakobsen", "therese johaug"]}, {"field": "kw-taxonomy", "value": ["sport", "sport/langrenn"]}, {"field": "og-image", "value": "http://www.adressa.no/incoming/a