ris."}, {"field": "dominantimage", "value": "http://www.adressa.no/incoming/article1395107.ece5/ALTERNATES/w980-default/nidar"}, {"field": "dominantimagedimensions", "value": "980x551"}, {"field": "dominantthumbnail", "value": "http://content-thumbnail.cxpublic.com/content/dominantthumbnail/94df9549a6450e6ce3e72a9861325e038d29ae0e.jpg?596da483"}, {"field": "dominantthumbnaildimensions", "value": "300x169"}, {"field": "heading", "value": "Kan mer enn \u00e5 lage drops og sjokolade -adressa.no"}, {"field": "id", "value": "94df9549a6450e6ce3e72a9861325e038d29ae0e"}, {"field": "keyword", "value": ["\u00f8konomi", "finans", "aksjer", "skatt", "b\u00f8rs", "\u00f8konominyheter"]}, {"field": "keywords", "value": "\u00f8konomi,finans,aksjer,skatt,b\u00f8rs,\u00f8konominyheter"}, {"field": "kw-category", "value": ["nyheter", "okonomi"]}, {"field": "kw-company", "value": "nidar"}, {"field": "kw-concept", "value": ["milj\u00f8pris", "redusert", "energi"]}, {"field": "kw-entity", "value": ["emil", "norsk energi"]}, {"field": "kw-pageclass", "value": "article"}, {"field": "kw-person", "value": "robert hjelmstad"}, {"field": "kw-taxonomy", "value": ["nyheter", "nyheter/okonomi"]}, {"field": "og-image", "value": "http://www.adressa.no/incoming/article1395107.ece5/ALTERNATES/w980-default/nidar"}, {"field": "og-locale", "value": "nb_NO"}, {"field": "og-site-name", "value": "adressa.no"}, {"field": "og-title", "value": "Kan mer enn \u00e5 lage drops og sjokolade"}, {"field": "og-type", "value": "article"}, {"field": "og-url", "value": "http://www.adressa.no/nyheter/okonomi/article1645799.ece"}, {"field": "pageclass", "value": ["article"]}, {"field": "publishtime", "value": "2011-06-09T22:39:32.000Z"}, {"field": "recs-articleid", "value": "1005651"}, {"field": "recs-category-type", "value": "taxonomy"}, {"field": "recs-category0", "value": "taxonomy|nyheter"}, {"field": "recs-category1", "value": "taxonomy|nyheter|okonomi"}, {"field": "recs-publishtime", "value": "2011-06-09T22:39:32.