tet."]}, {"field": "category0", "value": "sport"}, {"field": "category1", "value": "sport|hopp"}, {"field": "contenthash", "value": "98c5621b45480d5023f47f5d805d7f968edd020f9236f5faf2c8ef2c3e434dcd"}, {"field": "createtime", "value": "2017-03-01T16:05:37.000Z"}, {"field": "description", "value": "20 mil fra grensen mot Iran ligger vintersportsstedet som kan ta fra Gran\u00e5sen fremtidige verdenscuprenn i hopp."}, {"field": "dominantimage", "value": "http://www.adressa.no/incoming/article2117993.ece5/ALTERNATES/w980-default/Hoppbakkene-3"}, {"field": "dominantimagedimensions", "value": "980x551"}, {"field": "dominantthumbnail", "value": "http://content-thumbnail.cxpublic.com/content/dominantthumbnail/0dc1253fbdcad5f80d06e941e3347955235f6e69.jpg?58b6f151"}, {"field": "dominantthumbnaildimensions", "value": "300x169"}, {"field": "heading", "value": ["Tyrkia kan danke ut Gran\u00e5sen -adressa.no", "V-cup neste", "Vil bli vinternasjon"]}, {"field": "id", "value": "0dc1253fbdcad5f80d06e941e3347955235f6e69"}, {"field": "keyword", "value": ["nyheter", "trondheim", "ukeadressa", "eadressa", "skattelister", "d\u00f8dsfall", "tr\u00f8nder", "trafikk"]}, {"field": "keywords", "value": "nyheter,trondheim,ukeadressa,eadressa,skattelister,d\u00f8dsfall,tr\u00f8nder,trafikk"}, {"field": "kw-category", "value": ["sport", "hopp"]}, {"field": "kw-concept", "value": ["tyrkiske ungdommer", "arrangere verdenscuprenn", "vinter", "byen"]}, {"field": "kw-entity", "value": ["atakan alaftargil", "muthar kurt", "gran\u00e5sen", "erzurum universiaden"]}, {"field": "kw-location", "value": ["trondheim", "tyrkia", "iran", "ararat"]}, {"field": "kw-pageclass", "value": "article"}, {"field": "kw-taxonomy", "value": ["sport", "sport/hopp"]}, {"field": "og-image", "value": "http://www.adressa.no/incoming/article2117993.ece5/ALTERNATES/w980-default/Hoppbakkene-3"}, {"field": "og-locale", "value": "nb_NO"}, {"field": "og-site-name", "value": "adressa.no"}, {"field": "og-title", "value": "Tyrkia kan da