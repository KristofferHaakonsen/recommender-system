ode."]}, {"field": "category0", "value": "nyheter"}, {"field": "category1", "value": "nyheter|trondheim"}, {"field": "contenthash", "value": "1e9919d568df082a7a3ccaa0f3e6dfe63f68b0f7ee8ff2d7c4847db01d9b184e"}, {"field": "createtime", "value": "2017-08-02T11:56:28.000Z"}, {"field": "description", "value": "Sveisearbeid like f\u00f8r propangassflaske gikk i lufta."}, {"field": "dominantimage", "value": "http://www.adressa.no/incoming/article1405568.ece5/ALTERNATES/w980-default/Bynesveien%20Heiko%20Liebel%20167"}, {"field": "dominantimagedimensions", "value": "980x551"}, {"field": "dominantthumbnail", "value": "http://content-thumbnail.cxpublic.com/content/dominantthumbnail/6fe3a5634ef457fc783c00ae7f3b75b97d3d0d51.jpg?5981bdec"}, {"field": "dominantthumbnaildimensions", "value": "300x169"}, {"field": "heading", "value": ["Eksplosjon i Bynesveien -adressa.no", "Drev sveisearbeid i verkstedet", "Ingen personskader", "Teknisk unders\u00f8kelse"]}, {"field": "id", "value": "6fe3a5634ef457fc783c00ae7f3b75b97d3d0d51"}, {"field": "keyword", "value": ["utenriks", "innenriks", "trondheim", "E6", "midtbyen", "bybrann", "bilulykker"]}, {"field": "keywords", "value": "utenriks,innenriks,trondheim,E6,midtbyen,bybrann,bilulykker"}, {"field": "kw-category", "value": ["nyheter", "trondheim"]}, {"field": "kw-classification", "value": "law-government-and-politics"}, {"field": "kw-concept", "value": ["teknisk unders\u00f8kelse", "personer", "politi", "eksplosjon"]}, {"field": "kw-entity", "value": ["bynesveien 48", "trolla"]}, {"field": "kw-location", "value": ["s\u00f8r-tr\u00f8ndelag", "tr\u00f8ndelag"]}, {"field": "kw-pageclass", "value": "article"}, {"field": "kw-person", "value": ["geir arne haugen", "h\u00e5kon j\u00f8rgensen", "leif berglund"]}, {"field": "kw-sentiment", "value": "negative"}, {"field": "kw-taxonomy", "value": ["nyheter", "nyheter/trondheim"]}, {"field": "og-image", "value": "http://www.adressa.no/incoming/article1405568.ece5/ALTERNATES/w980-default/Bynesveien%20He