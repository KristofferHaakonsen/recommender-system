itikk"]}, {"field": "id", "value": "03f9ab9c917ee9088b9ca645c1cd453d8e0ae232"}, {"field": "keyword", "value": ["utenriks", "innenriks", "trondheim", "E6", "midtbyen", "bybrann", "bilulykker"]}, {"field": "keywords", "value": "utenriks,innenriks,trondheim,E6,midtbyen,bybrann,bilulykker"}, {"field": "kw-category", "value": ["nyheter", "nordtrondelag"]}, {"field": "kw-classification", "value": "law-government-and-politics"}, {"field": "kw-company", "value": ["stj\u00f8rdal kulturutvikling as", "reinertsen as"]}, {"field": "kw-concept", "value": ["sikrer gigantprosjektet", "kulturhusselskapet", "borgerlige partiene", "borgerlig", "stemme sikrer", "stemme sikrer gigantprosjektet", "stemme", "millioner kroner"]}, {"field": "kw-entity", "value": ["hent og skanska", "senterpartiet", "h\u00f8yre", "frp", "venstre og krf", "arbeiderpartiets og svs", "fremskrittspartiet"]}, {"field": "kw-location", "value": "midt-norge"}, {"field": "kw-pageclass", "value": "article"}, {"field": "kw-person", "value": ["arne frodthjof ven\u00e6s", "joar h\u00e5ve", "aps jan inge kaspersen", "ole hermod sandvik"]}, {"field": "kw-taxonomy", "value": ["nyheter", "nyheter/nordtrondelag"]}, {"field": "og-image", "value": "http://www.adressa.no/incoming/article3218350.ece/ALTERNATES/w980-default/Stj%C3%B8rdal%20kulturhus%20_%20fra%20s%C3%B8r%20vest.jpg"}, {"field": "og-locale", "value": "nb_NO"}, {"field": "og-site-name", "value": "adressa.no"}, {"field": "og-title", "value": "\u00c9n stemme sikrer gigantprosjektet"}, {"field": "og-type", "value": "article"}, {"field": "og-url", "value": "http://www.adressa.no/nyheter/nordtrondelag/article3218346.ece"}, {"field": "pageclass", "value": ["article"]}, {"field": "publishtime", "value": "2012-06-21T11:22:46.000Z"}, {"field": "recs-articleid", "value": "3218346"}, {"field": "recs-category-type", "value": "taxonomy"}, {"field": "recs-category0", "value": "taxonomy|nyheter"}, {"field": "recs-category1", "value": "taxonomy|nyheter|nordtrondelag"}, {"field": "r