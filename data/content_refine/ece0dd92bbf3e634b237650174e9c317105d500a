ag."}, {"field": "dominantimage", "value": "http://www.adressa.no/incoming/article8875103.ece/ALTERNATES/w980-default/IMG_0753.JPG"}, {"field": "dominantimagedimensions", "value": "980x551"}, {"field": "dominantthumbnail", "value": "http://content-thumbnail.cxpublic.com/content/dominantthumbnail/ece0dd92bbf3e634b237650174e9c317105d500a.jpg?595ff49d"}, {"field": "dominantthumbnaildimensions", "value": "300x169"}, {"field": "heading", "value": ["- Har aldri opplevd lignende -adressa.no", "Ingen tele", "F\u00f8rste og siste gang"]}, {"field": "id", "value": "ece0dd92bbf3e634b237650174e9c317105d500a"}, {"field": "keyword", "value": ["utenriks", "innenriks", "trondheim", "E6", "midtbyen", "bybrann", "bilulykker"]}, {"field": "keywords", "value": "utenriks,innenriks,trondheim,E6,midtbyen,bybrann,bilulykker"}, {"field": "kw-category", "value": ["nyheter", "nordtrondelag"]}, {"field": "kw-classification", "value": "hobbies-and-interests"}, {"field": "kw-company", "value": "frosta"}, {"field": "kw-concept", "value": ["opplevd lignende", "opplevd", "dag", "pl\u00f8ye"]}, {"field": "kw-entity", "value": ["stig arild og j\u00f8rgen oldervik", "revlan"]}, {"field": "kw-pageclass", "value": "article"}, {"field": "kw-person", "value": ["stig arild oldervik", "stig arild", "j\u00f8rgen oldervik"]}, {"field": "kw-taxonomy", "value": ["nyheter", "nyheter/nordtrondelag"]}, {"field": "og-image", "value": "http://www.adressa.no/incoming/article8875103.ece/ALTERNATES/w980-default/IMG_0753.JPG"}, {"field": "og-locale", "value": "nb_NO"}, {"field": "og-site-name", "value": "adressa.no"}, {"field": "og-title", "value": "- Har aldri opplevd lignende"}, {"field": "og-type", "value": "article"}, {"field": "og-url", "value": "http://www.adressa.no/nyheter/nordtrondelag/article8875107.ece"}, {"field": "pageclass", "value": ["article"]}, {"field": "publishtime", "value": "2013-12-28T20:21:15.000Z"}, {"field": "recs-articleid", "value": "8875107"}, {"field": "recs-category-type", "value": "taxonom