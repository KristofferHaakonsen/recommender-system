iden."}, {"field": "dominantimage", "value": "http://static.polarismedia.no/skins/prod/publication/adressa/gfx/applefavicon.png"}, {"field": "dominantimagedimensions", "value": "256x256"}, {"field": "dominantthumbnail", "value": "http://content-thumbnail.cxpublic.com/content/dominantthumbnail/a4031da3f3695bb5c741ca9b4213258be13f9120.jpg?598bc09d"}, {"field": "dominantthumbnaildimensions", "value": "256x256"}, {"field": "heading", "value": "Fant en halv milliard tyske mark i orgelet -adressa.no"}, {"field": "id", "value": "a4031da3f3695bb5c741ca9b4213258be13f9120"}, {"field": "keyword", "value": ["utenriks", "innenriks", "trondheim", "E6", "midtbyen", "bybrann", "bilulykker"]}, {"field": "keywords", "value": "utenriks,innenriks,trondheim,E6,midtbyen,bybrann,bilulykker"}, {"field": "kw-category", "value": ["nyheter", "innenriks"]}, {"field": "kw-concept", "value": ["halv", "tyske mark", "halv milliard", "milliard", "milliard tyske", "orgelet", "mark", "milliard tyske mark", "halv milliard tyske"]}, {"field": "kw-entity", "value": ["steinmeyer", "nidarosdomen", "adresseavisen"]}, {"field": "kw-location", "value": ["tyskland", "trondheim"]}, {"field": "kw-pageclass", "value": "article"}, {"field": "kw-person", "value": "august sieber"}, {"field": "kw-taxonomy", "value": ["nyheter", "nyheter/innenriks"]}, {"field": "og-image", "value": "http://static.polarismedia.no/skins/prod/publication/adressa/gfx/applefavicon.png"}, {"field": "og-locale", "value": "nb_NO"}, {"field": "og-site-name", "value": "adressa.no"}, {"field": "og-title", "value": "Fant en halv milliard tyske mark i orgelet"}, {"field": "og-type", "value": "article"}, {"field": "og-url", "value": "http://www.adressa.no/nyheter/innenriks/article8347417.ece"}, {"field": "pageclass", "value": ["article"]}, {"field": "publishtime", "value": "2013-09-27T07:56:14.000Z"}, {"field": "recs-articleid", "value": "8347417"}, {"field": "recs-category-type", "value": "taxonomy"}, {"field": "recs-category0", "value": "taxonom