et"}, {"field": "contenthash", "value": "6311d307b963f1c9c23f9b17b17d421cc2e6edeced33c1070e1f52e23518b934"}, {"field": "createtime", "value": "2015-11-24T08:47:14.000Z"}, {"field": "description", "value": "Trondheim opplever i \u00e5r sin mest sn\u00f8rike vinter siden 1985/86."}, {"field": "dominantimage", "value": "http://www.adressa.no/incoming/article1300486.ece5/ALTERNATES/w980-default/snoevaer.jpg"}, {"field": "dominantimagedimensions", "value": "980x551"}, {"field": "dominantthumbnail", "value": "http://content-thumbnail.cxpublic.com/content/dominantthumbnail/a51eaef3e03a4825408e5363e63296c95b54a6ae.jpg?5980da07"}, {"field": "dominantthumbnaildimensions", "value": "300x169"}, {"field": "heading", "value": ["Mest sn\u00f8rike vinter p\u00e5 23 \u00e5r -adressa.no", "Sn\u00f8dybden halvert"]}, {"field": "id", "value": "a51eaef3e03a4825408e5363e63296c95b54a6ae"}, {"field": "keyword", "value": ["v\u00e6ret", "nedb\u00f8r", "storm", "nina", "ivar", "hilde", "frida", "orkan"]}, {"field": "keywords", "value": "v\u00e6ret,nedb\u00f8r,storm,nina,ivar,hilde,frida,orkan"}, {"field": "kw-category", "value": "vaeret"}, {"field": "kw-concept", "value": ["vinter", "sn\u00f8dybde", "sn\u00f8rike vinter"]}, {"field": "kw-entity", "value": ["meteorologisk", "leinstrand"]}, {"field": "kw-location", "value": "trondheim"}, {"field": "kw-pageclass", "value": "article"}, {"field": "kw-taxonomy", "value": "vaeret"}, {"field": "og-image", "value": "http://www.adressa.no/incoming/article1300486.ece5/ALTERNATES/w980-default/snoevaer.jpg"}, {"field": "og-locale", "value": "nb_NO"}, {"field": "og-site-name", "value": "adressa.no"}, {"field": "og-title", "value": "Mest sn\u00f8rike vinter p\u00e5 23 \u00e5r"}, {"field": "og-type", "value": "article"}, {"field": "og-url", "value": "http://www.adressa.no/vaeret/article1285620.ece"}, {"field": "pageclass", "value": ["article"]}, {"field": "publishtime", "value": "2009-03-02T12:15:02.000Z"}, {"field": "recs-articleid", "value": "853207"}, {"f