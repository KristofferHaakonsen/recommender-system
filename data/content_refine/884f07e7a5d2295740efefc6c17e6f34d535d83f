nsatte."}, {"field": "dominantimage", "value": "http://static.polarismedia.no/skins/prod/publication/adressa/gfx/applefavicon.png"}, {"field": "dominantimagedimensions", "value": "256x256"}, {"field": "dominantthumbnail", "value": "http://content-thumbnail.cxpublic.com/content/dominantthumbnail/884f07e7a5d2295740efefc6c17e6f34d535d83f.jpg?58d2681c"}, {"field": "dominantthumbnaildimensions", "value": "256x256"}, {"field": "heading", "value": "Skatt Vest krever 18,6 millioner av Moods of Norway -adressa.no"}, {"field": "id", "value": "884f07e7a5d2295740efefc6c17e6f34d535d83f"}, {"field": "keyword", "value": ["utenriks", "innenriks", "trondheim", "E6", "midtbyen", "bybrann", "bilulykker"]}, {"field": "keywords", "value": "utenriks,innenriks,trondheim,E6,midtbyen,bybrann,bilulykker"}, {"field": "kw-category", "value": ["nyheter", "innenriks"]}, {"field": "kw-company", "value": "dagens n\u00e6ringsliv"}, {"field": "kw-concept", "value": ["rabatt", "skatt"]}, {"field": "kw-entity", "value": ["moods retail", "moods of norway", "brandstads", "skatt vest"]}, {"field": "kw-pageclass", "value": "article"}, {"field": "kw-person", "value": "jan egil flo"}, {"field": "kw-taxonomy", "value": ["nyheter", "nyheter/innenriks"]}, {"field": "og-image", "value": "http://static.polarismedia.no/skins/prod/publication/adressa/gfx/applefavicon.png"}, {"field": "og-locale", "value": "nb_NO"}, {"field": "og-site-name", "value": "adressa.no"}, {"field": "og-title", "value": "Skatt Vest krever 18,6 millioner av Moods of Norway"}, {"field": "og-type", "value": "article"}, {"field": "og-url", "value": "http://www.adressa.no/nyheter/innenriks/2016/07/14/Skatt-Vest-krever-186-millioner-av-Moods-of-Norway-13039680.ece"}, {"field": "pageclass", "value": ["article"]}, {"field": "publishtime", "value": "2016-07-14T17:40:40.000Z"}, {"field": "recs-articleid", "value": "13039680"}, {"field": "recs-category-type", "value": "taxonomy"}, {"field": "recs-category0", "value": "taxonomy|nyheter"}, {"field": "r