."]}, {"field": "category0", "value": "nyheter"}, {"field": "category1", "value": "nyheter|sortrondelag"}, {"field": "contenthash", "value": "c977a7837ca5218a16fe22f7a38f64223e5b81c1251e421b918484f390e64724"}, {"field": "createtime", "value": "2015-08-16T08:47:15.000Z"}, {"field": "description", "value": "- Det har v\u00e6rt for h\u00f8y temperatur p\u00e5 en sentral, sier Informasjonssjef i Telenor, Anders Krokan."}, {"field": "dominantimage", "value": "http://static.polarismedia.no/skins/prod/publication/adressa/gfx/applefavicon.png"}, {"field": "dominantimagedimensions", "value": "256x256"}, {"field": "dominantthumbnail", "value": "http://content-thumbnail.cxpublic.com/content/dominantthumbnail/0a2406c037175943a9321b27fde72e5f0deb2773.jpg?58c6d333"}, {"field": "dominantthumbnaildimensions", "value": "256x256"}, {"field": "heading", "value": "\u00c5rsak: For h\u00f8y temperatur -adressa.no"}, {"field": "id", "value": "0a2406c037175943a9321b27fde72e5f0deb2773"}, {"field": "keyword", "value": "S\u00f8r-Tr\u00f8ndelag politidistrikt"}, {"field": "keywords", "value": "S\u00f8r-Tr\u00f8ndelag politidistrikt"}, {"field": "kw-category", "value": ["nyheter", "sortrondelag"]}, {"field": "kw-company", "value": "telenor"}, {"field": "kw-concept", "value": ["h\u00f8y", "h\u00f8y temperatur", "feilen", "temperatur"]}, {"field": "kw-entity", "value": ["hitra og fr\u00f8ya", "informasjonssjef"]}, {"field": "kw-location", "value": "hitra"}, {"field": "kw-pageclass", "value": "article"}, {"field": "kw-person", "value": ["anders krokan", "trond volden"]}, {"field": "kw-taxonomy", "value": ["nyheter", "nyheter/sortrondelag"]}, {"field": "og-image", "value": "http://static.polarismedia.no/skins/prod/publication/adressa/gfx/applefavicon.png"}, {"field": "og-locale", "value": "nb_NO"}, {"field": "og-site-name", "value": "adressa.no"}, {"field": "og-title", "value": "\u00c5rsak: For h\u00f8y temperatur"}, {"field": "og-type", "value": "article"}, {"field": "og-url", "value": "http://www