 bekymret."}, {"field": "dominantimage", "value": "http://www.adressa.no/incoming/article12835430.ece/8r0mr6/ALTERNATES/w980-default/IMG_spd4908b.jpg_1_1_J6149FU.jpg"}, {"field": "dominantimagedimensions", "value": "980x551"}, {"field": "dominantthumbnail", "value": "http://content-thumbnail.cxpublic.com/content/dominantthumbnail/e06c504bcae992152a8e02bd70a5a64284b70e3d.jpg?5934f20e"}, {"field": "dominantthumbnaildimensions", "value": "300x169"}, {"field": "heading", "value": "Flere elger har f\u00e5tt d\u00f8delig sykdom -adressa.no"}, {"field": "id", "value": "e06c504bcae992152a8e02bd70a5a64284b70e3d"}, {"field": "keyword", "value": ["dyr", "Elg", "Selbu"]}, {"field": "keywords", "value": "dyr,Elg,Selbu"}, {"field": "kw-category", "value": ["nyheter", "sortrondelag"]}, {"field": "kw-classification", "value": "pets"}, {"field": "kw-concept", "value": ["sykdom", "elg", "d\u00f8delig sykdom"]}, {"field": "kw-entity", "value": ["d. det", "mattilsynet", "wasting disease"]}, {"field": "kw-location", "value": ["sogn og fjordane", "europa", "selbu", "s\u00f8r-tr\u00f8ndelag", "canada", "norge"]}, {"field": "kw-pageclass", "value": "article"}, {"field": "kw-person", "value": "kristina landsverk"}, {"field": "kw-taxonomy", "value": ["nyheter", "nyheter/sortrondelag"]}, {"field": "og-image", "value": "http://www.adressa.no/incoming/article12835430.ece/8r0mr6/ALTERNATES/w980-default/IMG_spd4908b.jpg_1_1_J6149FU.jpg"}, {"field": "og-locale", "value": "nb_NO"}, {"field": "og-site-name", "value": "adressa.no"}, {"field": "og-title", "value": "Flere elger har f\u00e5tt d\u00f8delig sykdom"}, {"field": "og-type", "value": "article"}, {"field": "og-url", "value": "http://www.adressa.no/nyheter/sortrondelag/2016/06/03/Flere-elger-har-f%C3%A5tt-d%C3%B8delig-sykdom-12835439.ece"}, {"field": "pageclass", "value": ["article"]}, {"field": "publishtime", "value": "2016-06-03T18:47:29.000Z"}, {"field": "recs-articleid", "value": "12835439"}, {"field": "recs-category-type", "value": "taxono