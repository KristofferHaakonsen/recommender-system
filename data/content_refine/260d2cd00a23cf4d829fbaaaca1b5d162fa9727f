et."]}, {"field": "category0", "value": "nyheter"}, {"field": "contenthash", "value": "06a698d660985010661d10888b1b1585ed194405151e8b26ecab86eb9bba4be1"}, {"field": "createtime", "value": "2017-05-01T06:57:08.000Z"}, {"field": "description", "value": "Norges f\u00f8rste kvinnelige ambassad\u00f8r til Storbritannia ble tatt imot p\u00e5 Buckingham Palace."}, {"field": "dominantimage", "value": "http://www.adressa.no/nyheter/article10436016.ece/wai1w/ALTERNATES/w980-default/mona%20juul%20dronning%20elizabeth.jpg"}, {"field": "dominantimagedimensions", "value": "980x551"}, {"field": "dominantthumbnail", "value": "http://content-thumbnail.cxpublic.com/content/dominantthumbnail/260d2cd00a23cf4d829fbaaaca1b5d162fa9727f.jpg?5981a461"}, {"field": "dominantthumbnaildimensions", "value": "300x169"}, {"field": "heading", "value": ["Mona Juul hilste p\u00e5 dronningen -adressa.no", "Hest og vogn", "\u2013 Milep\u00e6l"]}, {"field": "id", "value": "260d2cd00a23cf4d829fbaaaca1b5d162fa9727f"}, {"field": "keyword", "value": ["utenriks", "innenriks", "trondheim", "E6", "midtbyen", "bybrann", "bilulykker"]}, {"field": "keywords", "value": "utenriks,innenriks,trondheim,E6,midtbyen,bybrann,bilulykker"}, {"field": "kw-category", "value": "nyheter"}, {"field": "kw-classification", "value": "law-government-and-politics"}, {"field": "kw-concept", "value": ["kvinnelige ambassad\u00f8r", "dronningen", "ambassad\u00f8r", "kvinnen", "kvinnelige"]}, {"field": "kw-entity", "value": ["utenriksdepartementet", "international peace institute", "buckingham palace", "dagsrevyen", "sparbu"]}, {"field": "kw-location", "value": ["elizabeth", "storbritannia", "israel", "nord-tr\u00f8ndelag", "berlin", "norge", "new york"]}, {"field": "kw-pageclass", "value": "article"}, {"field": "kw-person", "value": ["jens stoltenbergs", "terje r\u00f8d-larsen", "mona juul"]}, {"field": "kw-taxonomy", "value": "nyheter"}, {"field": "og-image", "value": "http://www.adressa.no/nyheter/article10436016.ece/wai1w/ALTERNATES/