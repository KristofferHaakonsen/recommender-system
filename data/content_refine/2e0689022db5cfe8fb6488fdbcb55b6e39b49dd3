 sykehjem."]}, {"field": "category0", "value": "nyheter"}, {"field": "category1", "value": "nyheter|trondheim"}, {"field": "contenthash", "value": "401358884cf4e460ce10696a6e433f9f229ce9a8e3d6fb49093e33a586d0ebdf"}, {"field": "createtime", "value": "2017-02-28T13:29:08.000Z"}, {"field": "description", "value": "Men n\u00e5 har mysteriet f\u00e5tt en lykkelig slutt."}, {"field": "dominantimage", "value": "http://www.adressa.no/incoming/article7096697.ece/ALTERNATES/w980-default/stromaskin.JPG"}, {"field": "dominantimagedimensions", "value": "980x551"}, {"field": "dominantthumbnail", "value": "http://content-thumbnail.cxpublic.com/content/dominantthumbnail/2e0689022db5cfe8fb6488fdbcb55b6e39b49dd3.jpg?5995a9c1"}, {"field": "dominantthumbnaildimensions", "value": "300x169"}, {"field": "heading", "value": ["Str\u00f8maskin funnet forlatt p\u00e5 fortau -adressa.no", "Etterlyste eier p\u00e5 Twitter", "Gjenforent med eier"]}, {"field": "id", "value": "2e0689022db5cfe8fb6488fdbcb55b6e39b49dd3"}, {"field": "keyword", "value": ["utenriks", "innenriks", "trondheim", "E6", "midtbyen", "bybrann", "bilulykker"]}, {"field": "keywords", "value": "utenriks,innenriks,trondheim,E6,midtbyen,bybrann,bilulykker"}, {"field": "kw-category", "value": ["nyheter", "trondheim"]}, {"field": "kw-classification", "value": "pets"}, {"field": "kw-concept", "value": ["str\u00f8maskin", "str\u00f8maskin funnet forlatt", "funnet forlatt", "str\u00f8maskin funnet", "trondheim", "etterlyste eier", "str\u00f8maskina", "eier", "fortau"]}, {"field": "kw-entity", "value": ["hans lindgaard", "twitter", "e. c. dahls", "bispegata 9", "val\u00f8ya", "midtbyen", "adressa"]}, {"field": "kw-location", "value": ["s\u00f8r-tr\u00f8ndelag", "trondheim"]}, {"field": "kw-pageclass", "value": "article"}, {"field": "kw-person", "value": ["oddmund st\u00f8rseth", "bj\u00f8rn handegard"]}, {"field": "kw-taxonomy", "value": ["nyheter", "nyheter/trondheim"]}, {"field": "og-image", "value": "http://www.adressa.no/incoming/ar